import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int[] arr=new int[n];
        int i,f=0;
        for(i=0;i<n;i++){
            arr[i]=in.nextInt();
        }
        int val=in.nextInt();
        for(i=0;i<n;i++){
            if(arr[i]==val){
                f=1;
                break;}
        }
       if(f==1){
            System.out.println(val+" is presented in an array.");}
       else{
            System.out.println(val+" is not presented in an array.");}
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
