import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int[] arr=new int[n];
        int i,count=0,sum=0;
        for(i=0;i<n;i++){
            arr[i]=in.nextInt();
        }
        for(i=0;i<n;i++){
            if(arr[i]>0){
                count++;
            }
            else{
                sum++;
            }
        }
        System.out.printf("Count of Positive Integer is %.2f\n",(float)count);
         System.out.printf("Count of Negative Integer is %.2f",(float)sum);
        
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
