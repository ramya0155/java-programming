import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int a=in.nextInt();
        int b=in.nextInt();
        int c=in.nextInt();
        int d=((a*b)-(c*a))-100;
        System.out.println("Number of copies sold:"+a);
        System.out.println("Cost of each copy:"+b);
        System.out.println("Cost spent by agency on each newspaper:"+c);
        System.out.printf("The profit obtained is Rs.%.2f",(float)((a*b)-(c*a)-100));
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
